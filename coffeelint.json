    "no_interpolation_in_single_quotes": {
        "level": "warn"
    },
    "no_stand_alone_at": {
        "level": "warn"
    },
    "no_tabs": {
        "level": "warn"
    },
    "no_throwing_strings": {
        "level": "warn"
    },
    "no_trailing_semicolons": {
        "level": "warn"
